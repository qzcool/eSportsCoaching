<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper    
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.TestDao">
	<select id="classFind" resultType="string">
		select className from classes
	</select>
	
	<insert id="stdInsert" parameterType="Students">
		insert into students(code,name,age,sex,phone,classId) values 
		(#{code},#{name},#{age},#{sex},#{phone},#{classId})
	</insert>
</mapper>